{
  "source": {
    "include": ["./src/tasks"]
  },
  "plugins": [
    "plugins/markdown"
  ],
  "templates": {
    "referenceTitle": "grunt-ext",
    "collapse": true,
    "resources": {
      "github": "https://github.com/adinKent/grunt-ext"
    }
  },
  "opts": {
    "recurse": true,
    "destination": "./jsdoc/",
    "template": "node_modules/braintree-jsdoc-template"
  }
}
